<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tttare.management.mapper.CommonMapper">

    <delete id="deleteArticleTag" parameterType="int">
        delete from tb_article_tag where article_id = #{id}
    </delete>

    <delete id="deleteArticleCategory" parameterType="int">
        delete from tb_article_category where article_id = #{id}
    </delete>
</mapper>